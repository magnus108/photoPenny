{
  "name": "Example",
  "main": "electron.js",
  "dependencies": {
    "electron": "^1.6.1",
    "electron-packager": "^13.1.1",
    "freeport": "^1.0.5",
    "wait-on": "^2.0.2"
  },
  "scripts": {
    "package-mac": "electron-packager . --ignore=app --ignore=src --ignore=.stack-work/ --ignore=test --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --ignore=app --ignore=src --ignore=.stack-work/ --ignore=test --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron Tutorial App\"",
    "package-linux": "electron-packager . electron-tutorial-app --ignore=app --ignore=src --ignore=.stack-work/ --ignore=test --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  }
}
